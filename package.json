{
	"name": "xgrep",
	"displayName": "xgrep",
	"description": "Grep in the whole workspace and show results in the peek view instead of the file search view.",
	"publisher": "zjx20",
	"version": "4.1.0",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.xgrep.quickSearch",
		"onCommand:extension.xgrep.quickSearchPeek"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.xgrep.quickSearch",
				"title": "Quick Search"
			},
			{
				"command": "extension.xgrep.quickSearchPeek",
				"title": "Quick Search Peek"
			}
		],
		"keybindings": [
			{
				"command": "extension.xgrep.quickSearch",
				"key": "ctrl+shift+i",
				"mac": "cmd+shift+i"
			},
			{
				"command": "extension.xgrep.quickSearchPeek",
				"key": "ctrl+shift+j",
				"mac": "cmd+shift+j",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"https-proxy-agent": "^4.0.0",
		"proxy-from-env": "^1.1.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.32.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zjx20/vscode_xgrep.git"
	},
	"homepage": "https://github.com/zjx20/vscode_xgrep"
}
